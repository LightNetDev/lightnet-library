  },
  "bugs": {
    "url": "https://github.com/LightNetDev/lightnet/issues"
  },
  "homepage": "https://lightnet.community",
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@eslint/js": "^9.21.0",
    "astro": "^5.4.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
